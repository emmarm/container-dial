{
  "manifest_version": 1,
  "short_name": "container-dial",
  "name": "Container Dial Web Extension",
  "version": "0.1.0",
  "icons": [
    {
      "src": "favicon.ico",
      "sizes": "64x64 32x32 24x24 16x16",
      "type": "image/x-icon"
    }
  ],
  "chrome_settings_overrides": {
    "homepage": "index.html"
  },
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "permissions": ["contextualIdentities", "cookies", "storage", "tabs"],
  "applications": {
    "gecko": {
      "id": "containerdial@firefoxaddons.org",
      "strict_min_version": "54.0a1"
    }
  },
  "start_url": "./index.html",
  "display": "standalone",
  "theme_color": "#000000",
  "background_color": "#ffffff"
}
